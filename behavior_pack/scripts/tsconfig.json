{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "lib": ["ES2020"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./",
        "rootDir": "./src",
        "declaration": false,
        "sourceMap": false
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}
